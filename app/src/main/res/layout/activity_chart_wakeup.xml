<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context=".activities.ChartActivity">

        <TextView
            android:id="@+id/sleep_chart_activity_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="Sleep chart" />

        <com.og.health.sleeptracker.views.ProgressLineChartView
            android:id="@+id/sleep_chart_activity_linechart"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_below="@+id/sleep_chart_activity_textview"
            android:layout_centerInParent="true" />

        <TextView
            android:id="@+id/wakeup_chart_activity_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sleep_chart_activity_linechart"
            android:layout_centerHorizontal="true"
            android:text="Wake up chart" />

        <com.og.health.sleeptracker.views.ProgressLineChartView
            android:id="@+id/wakeup_chart_activity_linechart"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_below="@+id/wakeup_chart_activity_textview"
            android:layout_centerInParent="true" />
    </RelativeLayout>
</layout>
